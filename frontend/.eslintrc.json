{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "import/resolver": {
          "node": {
            "paths": ["src"]
          }
        }
      },    
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
        "quotes": [
            "error",
            "double"
        ],
        "no-shadow": "off",
        "react/prop-types": "off",
        "no-underscore-dangle": "off"
    }
}